@model IEnumerable<UnsignedDocument>
@{
    ViewData["ShowSignButton"] = true;
}

<partial name="_Sign-Unsign_Nav-bar" />


<h4 class="display-4">Unsigned Documents</h4>


<a class="btn btn-outline-primary" style="margin-bottom:15px; margin-top:7px" asp-controller="SignDocuments" asp-action="SignDocument">Sign form disk</a>

@{
    foreach (var unsignedDocuments in Model)
    {

        <partial name="_UnsignedDocumentCard" model="unsignedDocuments" view-data="ViewData" />
    }
}