@model IEnumerable<UnsignedDocument>

@{ 
   foreach (var udoc in Model)
   {

    <div class="card">
        <div class="card-header">
            @udoc.Name
        </div>
        <div class="card-body">
            <h5 class="card-title">Special title treatment</h5>
            <p class="card-text">Key Preference: @udoc.KeyPreference </p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
    
   }
}



