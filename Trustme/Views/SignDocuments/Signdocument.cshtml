@model Trustme.Models.Key
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Sign Documents";
}
<h1>Generate certificates</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GenerateCertificate">
            <div class="col text-center">
                <input type="submit" value="Generate Cerificate and private key" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<hr />

<form asp-action="SignDoc" enctype="multipart/form-data" method="post">
    <dl class="row">
        <dt>
            <label for="pkfile">Private key</label>
            <input for="pkfile" name="pkfile" type="file">

        </dt>
        <dt>
            <label for="docfile">Document</label>
            <input for="docfile" name="docfile" type="file">
        </dt>

    </dl>
        <input type="submit" value="Upload" class="btn btn-primary" />
</form>


