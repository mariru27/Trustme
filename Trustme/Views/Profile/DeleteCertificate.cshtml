@model Key

@{
    ViewData["Title"] = "Delete Certificate";
}
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Key</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CertificateName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CertificateName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>

    <form asp-action="DeleteCertificate">
        <input type="hidden" asp-for="KeyId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Profile">Back to Profile</a>
    </form>
</div>
