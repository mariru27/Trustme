@model IEnumerable<SignedDocumentsViewModel>

<h4 class="display-4">Signed documents</h4>



@{
    foreach (var udoc in Model)
    {
        <div class="card">
            <div class="card-header">
                Signed by username:
                @udoc.SignedByUsername
            </div>
            <div class="card-body">
                <div class="form-group">
                    File Name:
                    @udoc.Name
                </div>
                <div class="form-group">
                    Signature:
                    <textarea class="form-control" name="Signature">@udoc.Signature</textarea>

                </div>
            </div>
        </div>
        <br>

        }
    }